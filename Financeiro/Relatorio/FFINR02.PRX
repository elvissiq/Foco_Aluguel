        oExcel:Img(nIDImg,nLin,1,040,035,"px",)
    EndIf
    oExcel:Pos(nLin,2):SetValue("OUTROS"):SetStyle(oEstCNVer)

    For nY := 1 To Len(aDays)

        nColuna := (nY+2)    
